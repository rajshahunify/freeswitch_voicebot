[Unit]
Description=FreeSWITCH VoiceBot Server
After=network.target freeswitch.service

[Service]
Type=simple
User=root
WorkingDirectory=/mnt/c/Users/unify/freeswitch_voicebot
ExecStart=/mnt/c/Users/unify/freeswitch_voicebot/venv/bin/python server.py
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target
